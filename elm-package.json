{
    "version": "1.0.0",
    "summary": "Experimental library for parsing ABC files",
    "repository": "https://github.com/newlandsvalley/elm-abc-parser.git",
    "license": "BSD3",
    "source-directories": [
        "src"
    ],
    "exposed-modules": [
        "Abc",
        "Abc.Canonical",
        "Abc.ParseTree",
        "Music.Notation",
        "Music.Octave",
        "Music.Transposition"
    ],
    "native-modules": false,   
    "dependencies": {
        "elm-lang/core": "4.0.0 <= v < 5.0.0",
        "Bogdanp/elm-combine": "2.2.1 <= v < 3.0.0",
        "imeckler/ratio": "2.0.1 <= v < 3.0.0",
        "elm-community/maybe-extra": "1.0.0 <= v < 2.0.0",
        "elm-community/elm-list-extra": "1.0.0 <= v < 2.0.0"
    },
    "elm-version": "0.17.0 <= v < 0.18.0"
}

