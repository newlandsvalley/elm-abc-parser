        "elm-lang/core": "3.0.0 <= v < 4.0.0",
        "Bogdanp/elm-combine": "2.0.2 <= v < 3.0.0",
        "imeckler/ratio": "2.0.0 <= v < 3.0.0",
        "circuithub/elm-maybe-extra": "1.6.0 <= v < 2.0.0",
        "circuithub/elm-list-extra": "3.9.0 <= v < 4.0.0"
    },
    "elm-version": "0.16.0 <= v < 0.17.0"
}

